ImageLabel.Size = UDim2.new(0, 243, 0, 199)
ImageLabel.Image = "http://www.roblox.com/asset/?id=7715783560"

TextLabel.Parent = ImageLabel
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.BackgroundTransparency = 1.000
TextLabel.Position = UDim2.new(0.0856910646, 0, 0.744422734, 0)
