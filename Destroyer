local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Slap Battles ez bypass", "Synapse")
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("LocalPlayer")
MainSection:NewSlider("WalkSpeed", "Make You Walk Faster", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
MainSection:NewButton("Anti Ragdoll", "bypassing ragdoll (flying glitch)", function()
    local ragdoll = game:GetService("Players").LocalPlayer.Character.isInArena
	ragdoll:Remove()
end)
MainSection:NewButton("Default Arena AntiRagdoll", "bypassing ragdoll (flying glitch)", function()
    local ragdoll = game:GetService("Players").LocalPlayer.Character.IsInDefaultArena
	ragdoll:Remove()
end)
end)
Misc:NewButton("Unlock Workspace", "unlock locked part", function()
	for i,v in pairs(workspace:GetDescendants()) do
		if v:IsA("BasePart") then
			v.Locked = false
		end
	end
end)

LocalPlayer:NewToggle("Inf Jump", "You Will Jump Inf", function(state)
	local Player = game:GetService("Players").LocalPlayer
	local Mouse = Player:GetMouse()
	Mouse.KeyDown:connect(function(k)
		if _G.infinjump then
			if k:byte() == 32 then
				Humanoid = game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
				Humanoid:ChangeState("Jumping")
				wait(0.1)
				Humanoid:ChangeState("Seated")
			end
		end
	end) 

if state then
        _G.infinjump = true
    else
        _G.infinjump = false
    end
end)
LocalPlayer:NewButton("The Wok Give Social Credit", "haha yes u will get social credit", function()
    local ScreenGui = Instance.new("ScreenGui")
local ImageLabel = Instance.new("ImageLabel")
local TextLabel = Instance.new("TextLabel")
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
