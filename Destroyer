MainSection:NewButton("Fe Checker", "detect if game had FE on or not", function()
    if Workspace.FilteringEnabled == true then
      game.StarterGui:SetCore("SendNotification", {
    Title = 'FE Is Enabled!';
    Text = 'Now Fuck Off This Useless Feature';
    Duration = 6;
  })
    else
      game.StarterGui:SetCore("SendNotification", {
    Title = 'FE Is Disabled!';
    Text = 'Now Go Play Other Game';
    Duration = 6;
  })
end
end)
LocalPlayer:NewButton("Netless Remake", "This only work when the script dosent have netless/bypass", function()
settings().Physics.AllowSleep = false
for i,v in next, game:GetService("Players").LocalPlayer.Character:GetDescendants() do
if v:IsA("BasePart") and v.Name ~="HumanoidRootPart" then 
game:GetService("RunService").Heartbeat:connect(function()
v.Velocity = Vector3.new(0,35,0)
wait(1)
end)
end
end
end)
Option:NewButton("Lower Graphic", "Boost FPS And Less Stutter", function(args, speaker)
    	workspace:FindFirstChildOfClass('Terrain').WaterWaveSize = 0
	workspace:FindFirstChildOfClass('Terrain').WaterWaveSpeed = 0
	workspace:FindFirstChildOfClass('Terrain').WaterReflectance = 0
	workspace:FindFirstChildOfClass('Terrain').WaterTransparency = 0
	game:GetService("Lighting").GlobalShadows = false
	game:GetService("Lighting").FogEnd = 9e9
	settings().Rendering.QualityLevel = 1
		for i,v in pairs(game:GetService("Lighting"):GetDescendants()) do
		if v:IsA("BlurEffect") or v:IsA("SunRaysEffect") or v:IsA("ColorCorrectionEffect") or v:IsA("BloomEffect") or v:IsA("DepthOfFieldEffect") then
			v.Enabled = false
		end
	end
end)
